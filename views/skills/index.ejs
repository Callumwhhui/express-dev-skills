<%- include('../partials/header') %>
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
            <form action="/skills/<%= s.id %>?_method=DELETE" method="post" class="delete-form">
                <button type="submit">x</button>
            </form>
          <a href="/skills/<%= s.id %>"> <%= s.skill %> </a>
            - 
          <%= s.learned ? 'Yes' : 'not learned yet' %>
        </li>
      <% }); %>
    </ul>
    <a href="/skills/new">Add skill</a>

    <%- include('../partials/footer') %>